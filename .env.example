# Master node IP (ActiveMQ + Nginx)
MASTER_NODE_IP=10.26.14.202

# This node's physical IP
CURRENT_NODE_IP=10.26.14.X

# Worker node IPs in the lab.
# Ports are expanded automatically (defaults to Hazelcast member ports 5701 and 5702).
CLUSTER_NODES_LIST=10.26.14.200,10.26.14.201,10.26.14.202

# Optional override of which member ports should be tried for each IP.
# Leave unset unless you change the default Hazelcast ports.
# hazelcast.member.ports=5701,5702

# Keep this identical on all nodes
HAZELCAST_CLUSTER_NAME=search-cluster

# Host path to this node's local datalake directory
HOST_DATA_VOLUME_PATH=./datalake

# Enable/disable crawler-to-crawler replication (R=2) at the application level
DATALAKE_REPLICATION_ENABLED=true
