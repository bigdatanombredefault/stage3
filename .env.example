# Master node IP (ActiveMQ + Nginx)
MASTER_NODE_IP=XXX

# This node's physical IP
CURRENT_NODE_IP=XXX

# Comma-separated list of all nodes in the lab
CLUSTER_NODES_LIST=XXX,YYY,ZZZ

# Host path to this node's local datalake directory
HOST_DATA_VOLUME_PATH=/mnt/datalake

# Enable/disable crawler-to-crawler replication (R=2) at the application level
DATALAKE_REPLICATION_ENABLED=true
